<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.PlaceholderFragmentTasks.PlaceholderFragment_Task_0102">

    <TextView
        android:id="@+id/textView6_task"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="и"
        android:textColor="#0A0A0A"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/editText3_task2"
        app:layout_constraintTop_toTopOf="@+id/editText_task2" />

    <TextView
        android:id="@+id/textView6_task2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="значное"
        android:textColor="#0A0A0A"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/editText4_task2"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText3_task2"
        android:layout_width="64dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="n-нулей"
        android:inputType="number"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4_task2" />

    <TextView
        android:id="@+id/textView_task2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Укажите"
        android:textColor="#0A0A0A"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2_task2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:gravity="center"
        android:text="наименьшее"
        android:textColor="#858484"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_task2"
        app:layout_constraintStart_toEndOf="@+id/textView_task2" />

    <TextView
        android:id="@+id/textView4_task2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:gravity="center"
        android:text="шестнадцатеричное число двочиная запись которого содержит ровно"
        android:textAlignment="viewStart"
        android:textColor="#0A0A0A"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText4_task2" />

    <EditText
        android:id="@+id/editText_task2"
        android:layout_width="86dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:gravity="center"
        android:hint="n-едениц"
        android:inputType="textPersonName"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView6_task"
        app:layout_constraintTop_toBottomOf="@+id/textView4_task2" />

    <Button
        android:id="@+id/button_task2"
        android:layout_width="83dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="решить"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText3_task2" />

    <TextView
        android:id="@+id/tAnswer_task2"
        android:layout_width="85dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:gravity="center"
        android:text="Ответ: "
        android:textSize="16sp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/button_task2"
        app:layout_constraintTop_toTopOf="@+id/button_task2" />

    <EditText
        android:id="@+id/editText4_task2"
        android:layout_width="42dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:gravity="center"
        android:hint="x"
        android:inputType="number"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView2_task2"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>